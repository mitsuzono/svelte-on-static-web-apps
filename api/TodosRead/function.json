{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "todos"
    },
    {
      "name": "documents",
      "type": "cosmosDB",
      "direction": "in",
